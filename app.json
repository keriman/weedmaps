{
  "expo": {
    "name": "Dispensarios Médicos",
    "slug": "dispensarios-medicos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mercadoWeed.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mercadoWeed.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.dispensariosmedicos",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "La aplicación necesita acceso a tu ubicación para mostrar dispensarios cercanos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mercadoWeed.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.dispensariosmedicos",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/mercadoWeed.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir a Dispensarios Médicos acceder a tu ubicación."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "295e5f76-8519-4763-98ed-5ab48bb8bba3"
      }
    }
  }
}
